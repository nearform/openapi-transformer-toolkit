{
  "required": [
    "name",
    "photoUrls"
  ],
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64",
      "minimum": -9223372036854776000,
      "maximum": 9223372036854776000
    },
    "name": {
      "type": "string"
    },
    "category": {
      "$ref": "Category.json"
    },
    "photoUrls": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "$ref": "Tag.json"
      }
    },
    "status": {
      "type": "string",
      "description": "pet status in the store",
      "enum": [
        "available",
        "pending",
        "sold"
      ]
    },
    "nullableValue": {
      "type": [
        "string",
        "null"
      ],
      "description": "example nullable value"
    }
  },
  "title": "Pet",
  "$id": "components.schemas/Pet.json"
}